{
  "name": "nobackend.website",
  "version": "1.0.0",
  "description": "A powerful rich interactive web application by using nobackend tech stack.",
  "main": "./app/app.js",
  "scripts": {
    "dev:ng": "webpack-dev-server --config webpack.angular.config.js",
    "build:ng": "cross-env NODE_ENV=production webpack --config webpack.angular.config.js --progress --colors --hide-modules",
    "dev:vue": "webpack-dev-server --config webpack.vue.config.js",
    "build:vue": "cross-env NODE_ENV=production webpack --config webpack.vue.config.js --progress --colors --hide-modules",
    "dev:react": "webpack-dev-server --config webpack.react.config.js",
    "build:react": "cross-env NODE_ENV=production webpack --config webpack.react.config.js --progress --colors --hide-modules",
    "lint:react": "eslint -c .eslintrc.json --ext .jsx --ext .js src/react/*",
    "test:react": "karma start --single-run=true",
    "prod:react": "npm run lint:react && npm run test:react && npm run build:react && cd scripts && ./qrsync-v2 _react.conf.json",
    "cdn": "cd scripts && ./qrsync-v2 _ng.conf.json && ./qrsync-v2 _vue.conf.json && ./qrsync-v2 _react.conf.json",
    "clean": "rm -rf _ng _vue _react _site",
    "build": "npm run clean && npm run build:ng && npm run build:vue && npm run build:react",
    "codecov": "cat coverage/*/lcov.info | codecov",
    "prod": "npm run build && npm run cdn",
    "start": "jekyll clean && jekyll serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JimmyLv/nobackend.website"
  },
  "homepage": "http://nobackend.website/",
